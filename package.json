{
  "name": "ember-exam",
  "version": "0.3.1",
  "description": "Run your tests with randomization, splitting, and parallelization for beautiful tests.",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "mocha --recursive node-tests/",
    "test:ember": "ember try:testall",
    "test:all": "npm test && npm run test:ember",
    "test:ci": "npm test && ember try $EMBER_TRY_SCENARIO test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trentmwillis/ember-exam.git"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "broccoli-asset-rev": "^2.2.0",
    "ember-ajax": "0.7.1",
    "ember-cli": "2.4.2",
    "ember-cli-app-version": "^1.0.0",
    "ember-cli-dependency-checker": "^1.2.0",
    "ember-cli-htmlbars": "^1.0.1",
    "ember-cli-htmlbars-inline-precompile": "^0.3.1",
    "ember-cli-inject-live-reload": "^1.3.1",
    "ember-cli-qunit": "^1.2.1",
    "ember-cli-release": "0.2.8",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-disable-proxy-controllers": "^1.0.1",
    "ember-export-application-global": "^1.0.4",
    "ember-load-initializers": "^0.5.0",
    "ember-resolver": "^2.0.3",
    "ember-try": "~0.2.0",
    "loader.js": "^4.0.0",
    "mocha": "^2.3.4"
  },
  "keywords": [
    "ember-addon"
  ],
  "dependencies": {
    "ember-cli-babel": "^5.1.5",
    "recast": "^0.10.33",
    "rsvp": "^3.1.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
